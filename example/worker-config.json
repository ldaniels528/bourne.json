{
  "baseDirectory": "./example",
  "maxConcurrency": 2,
  "triggers": [
    {
      "name": "UserAgentsTSV",
      "workflowName": "UserAgentsTSV.json",
      "patterns": [
        "useragents*.tsv"
      ]
    },
    {
      "name": "UserAgentsJSON",
      "workflowName": "UserAgentsJSON.json",
      "patterns": [
        "useragents*.json"
      ]
    },
    {
      "name": "UserAgentsFIXED",
      "workflowName": "UserAgentsFIXED.json",
      "patterns": [
        "useragents*.csv"
      ]
    },
    {
      "name": "ListingActivityTxt",
      "workflowName": "ListingActivityTxt.json",
      "patterns": [
        "LISTING_ACTIVITY*.txt",
        "LISTING_ACTIVITY*.txt.gz"
      ]
    }
  ]
}